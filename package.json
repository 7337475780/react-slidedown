{
  "name": "react-slidein",
  "version": "1.2.0",
  "description": "Component for animating height during mount/unmount using a CSS transition",
  "main": "lib/slidein.js",
  "style": "lib/slidein.css",
  "scripts": {
    "test": "npm run build && karma start --single-run",
    "test:watch": "karma start",
    "test:start": "concurrently \"npm run build:watch\" \"npm run test:watch\" --kill-others",
    "build": "tsc",
    "build:watch": "tsc -w",
    "example:watch": "watchify ./example/index.js -o ./example/bundle.js",
    "example:serve": "live-server --open=./example --port=5555",
    "start": "concurrently \"npm run build:watch\" \"npm run example:watch\" \"npm run example:serve\" --kill-others"
  },
  "author": "Frank Wallis",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/enzyme": "^3.1.0",
    "@types/react": "16.0.16",
    "@types/react-dom": "^16.0.2",
    "browserify": "^14.1.0",
    "chai": "^4.1.2",
    "concurrently": "^3.4.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "karma": "^1.5.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^4.0.1",
    "purecss": "^1.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "typescript": "^2.5.3",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "react-transition-group": "^1.2.0"
  },
  "files": [
    "lib/slidein.js",
    "lib/slidein.css",
    "lib/slidein.js.map"
  ],
  "repository": "frankwallis/react-slidein",
  "keywords": [
    "react",
    "animation",
    "transition",
    "mount",
    "height",
    "auto",
    "dynamic",
    "css",
    "slide"
  ]
}
